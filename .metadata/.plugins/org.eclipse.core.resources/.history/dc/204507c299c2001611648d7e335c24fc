package com.github.string;

public class Demo2_Method {
	public static void main(String[] args) {
		String s1 = "coppco";
		byte[] arr = s1.getBytes();
		
		for (int i = 0; i < arr.length; i++) {
			System.out.println(arr[i]);
		}
		s1 = "中文试试, 中文可以吗i";
		arr = s1.getBytes();
		for (int i = 0; i < arr.length; i++) {
			System.out.println(arr[i]);
		}
	}
}
