package com.github.coppco;

public class Demo1_Thread {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		MyThread thread = new MyThread();
		thread.start();
		while (true) {
			System.out.println("2");
		}
	}

}

/*
 * ий╩й1
 */
class MyThread extends Thread {
	@Override
	public void run() {
		while (true) {
			System.out.println("1");
		}
	}
}