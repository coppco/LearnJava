package com.github.string;

public class HJ_String_Mothed1 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		String s1 = "coppco";
		String s2 = "这里是中文哦!";
		String s3 = "";
		System.out.println(s1.length());
		System.out.println(s2.length());
		System.out.println(s3.length());
		
		String s4 = "coppco";
		char c = s4.charAt(8);
		System.out.println(c);
		
	}

}
