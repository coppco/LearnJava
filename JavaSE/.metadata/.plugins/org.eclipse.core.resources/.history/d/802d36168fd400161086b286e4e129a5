package com.github.coppco;

public class Demo3_ThreadMothed {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		new Thread() {
			@Override
			public void run() {
				System.out.println(this.getName());
			}
		}.start();
		new Thread() {
			@Override
			public void run() {
				System.out.println(this.getName());
			}
		}.start();
	}

}
